{
  "name": "cra-ci-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "@snyk/protect": "latest"
  },
  "devDependencies": {
    "@size-limit/preset-big-lib": "^4.5.0",
    "cypress": "^4.5.0",
    "size-limit": "^4.5.0",
    "start-server-and-test": "^1.11.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js --max-warnings 0",
    "size": "yarn build && size-limit",
    "cypress:run": "cypress run --record --key=${CYPRESS_RECORD_KEY}",
    "ci:cypress-run": "start-server-and-test start http://localhost:3000 cypress:run",
    "prepare": "yarn run snyk-protect",
    "snyk-protect": "snyk-protect"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "size-limit": [
    {
      "limit": "1.5 s",
      "path": "build/static/**/*.{js,css,svg,woff,woff2,png,ttf,html}"
    }
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "snyk": true
}
